                <property name="styleSheet">
                 <string notr="true">/* 1. ÜÇ BUTON İÇİN ORTAK TEMEL AYARLAR */
QPushButton#HesaplarimveBakiyeButon,
QPushButton#IslemGecmisiButon,
QPushButton#PortfoyDurumuButon {
    border: none;
	background:none;
    background-color: transparent; /* Normal halde şeffaf */
    color: rgb(129, 129, 129);
	text-color:rgb(0,0,0);
    font-size: 14px;
    font-weight: 500;
    padding-bottom: 15px;
    padding-top: 15px;
    border-bottom: 3px solid transparent;
    text-align: left;
}

/* 2. MOUSE ÜZERİNE GELİNCE (HOVER) */
QPushButton#HesaplarimveBakiyeButon:hover,
QPushButton#IslemGecmisiButon:hover,
QPushButton#PortfoyDurumuButon:hover {
    color: #101828;
    background-color: transparent; /* Hover'da da arka plan gelmesin */
}

/* 3. BUTONA BASILI TUTULDUĞUNDA (PRESSED) - Sorunu Çözen Kısım */
QPushButton#HesaplarimveBakiyeButon:pressed,
QPushButton#IslemGecmisiButon:pressed,
QPushButton#PortfoyDurumuButon:pressed {
    background-color: transparent; /* Basınca mavi kutu çıkmasını engeller */
}

/* 4. BUTON SEÇİLİ OLDUĞUNDA (CHECKED) - Sorunu Çözen Kısım */
QPushButton#HesaplarimveBakiyeButon:checked,
QPushButton#IslemGecmisiButon:checked,
QPushButton#PortfoyDurumuButon:checked {
    color: #2b5cff;
    border-bottom: 3px solid #2b5cff;
    background-color: transparent; /* Seçiliyken arka planı şeffaf zorla */
}</string>
